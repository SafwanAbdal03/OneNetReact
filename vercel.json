{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "public/index.html"
    },
    {
      "src": "/src/App.js",
      "dest": "src/App.js"
    },
    {
      "src": "/api/data",
      "dest": "/api/data"
    }
  ]
}




  